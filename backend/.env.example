# Azure Storage
AZURE_STORAGE_CONNECTION_STRING=your-storage-connection-string
AZURE_STORAGE_ACCOUNT_NAME=your-storage-account-name
AZURE_STORAGE_ACCOUNT_KEY=your-storage-account-key

# Azure Cosmos DB
COSMOS_DB_CONNECTION_STRING=your-cosmos-connection-string
COSMOS_DB_NAME=your-cosmos-db-name

# Neo4j
NEO4J_URI=neo4j://localhost:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=your-password

# StarRocks
STARROCKS_HOST=localhost
STARROCKS_PORT=9030
STARROCKS_USER=root
STARROCKS_PASSWORD=your-password

# Redis Cache
REDIS_CONNECTION_STRING=your-redis-connection-string
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=your-password

# Azure AD B2C
AZURE_AD_B2C_TENANT_NAME=your-tenant-name
AZURE_AD_B2C_CLIENT_ID=your-client-id
AZURE_AD_B2C_CLIENT_SECRET=your-client-secret

# Azure Key Vault
AZURE_KEY_VAULT_NAME=your-key-vault-name
AZURE_KEY_VAULT_URI=https://your-key-vault.vault.azure.net/

# AI Services
OPENAI_API_KEY=your-openai-api-key
AZURE_OPENAI_API_KEY=your-azure-openai-api-key
AZURE_OPENAI_ENDPOINT=your-azure-openai-endpoint

# Application Settings
ENVIRONMENT=development
LOG_LEVEL=INFO
ENABLE_TELEMETRY=false
MAX_BATCH_SIZE=1000
CACHE_TTL=3600